{
    "extends": "@vue/tsconfig/tsconfig.web.json",
    "include": [
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue"
    ],
    "compilerOptions": {
        "target": "ES2019",
        "lib": ["ES2019"],
        "skipLibCheck": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
    "references": [
        {
            "path": "./tsconfig.vite-config.json"
        }
    ]
}
